							"markers": []
						},
						"layout": {
							"x": 0,
							"y": 0,
							"width": 6,
							"height": 3
						}
					},
					{
						"definition": {
							"title": "Latency on store-frontend",
							"title_size": "16",
							"title_align": "left",
							"show_legend": false,
							"time": {},
							"type": "distribution",
							"xaxis": {
								"max": "p95",
								"min": "auto"
							},
							"yaxis": {
								"max": "auto",
								"scale": "linear",
								"min": "auto"
