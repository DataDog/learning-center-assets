		},
		{
			"definition": {
				"title": "Errors",
				"type": "group",
				"background_color": "pink",
				"show_title": true,
				"layout_type": "ordered",
				"widgets": [
					{
						"definition": {
							"title": "Errors on store-frontend",
							"title_size": "16",
							"title_align": "left",
							"show_legend": false,
							"legend_layout": "auto",
							"legend_columns": [
								"avg",
								"min",
								"max",
								"value",
								"sum"
							],
							"time": {},
							"type": "timeseries",
							"requests": [
								{
									"queries": [
										{
											"data_source": "metrics",
											"name": "query1",
											"query": "sum:trace.rack.request.errors{env:im-workshop,service:store-frontend} by {http.status_code}.as_count()"
										}
									],
									"response_format": "timeseries",
									"style": {
										"palette": "warm"
