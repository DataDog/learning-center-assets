		{
			"definition": {
				"title": "All Services",
				"type": "group",
				"background_color": "green",
				"show_title": true,
				"layout_type": "ordered",
				"widgets": [
					{
						"definition": {
							"title": "% of Time Spent by All Services on store-frontend",
							"title_size": "16",
							"title_align": "left",
							"show_legend": false,
							"legend_layout": "auto",
							"legend_columns": [
								"avg",
								"min",
								"max",
								"value",
								"sum"
							],
							"time": {},
							"type": "timeseries",
							"requests": [
								{
									"formulas": [
										{
											"formula": "query1 * 100 / query2"
										}
									],
									"queries": [
										{
											"data_source": "metrics",
											"name": "query1",
											"query": "sum:trace.rack.request.exec_time.by_service{env:im-workshop,service:store-frontend} by {sublayer_service}.rollup(sum).fill(zero)"
										},
										{
											"data_source": "metrics",
											"name": "query2",
											"query": "sum:trace.rack.request.exec_time.by_service{env:im-workshop,service:store-frontend}.rollup(sum).fill(zero)"
										}
									],
									"response_format": "timeseries",
									"display_type": "area"
								}
							],
							"yaxis": {
								"max": "100"
							},
							"markers": []
						},
						"layout": {
							"x": 0,
							"y": 0,
							"width": 12,
							"height": 3
						}
					}
				]
			},
			"layout": {
				"x": 0,
				"y": 8,
				"width": 12,
				"height": 4
			}
