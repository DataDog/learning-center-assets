							"markers": []
						},
						"layout": {
							"x": 0,
							"y": 0,
							"width": 6,
							"height": 3
						}
					},
					{
						"definition": {
							"title": "Requests and Errors on store-frontend",
							"title_size": "16",
							"title_align": "left",
							"show_legend": false,
							"legend_layout": "auto",
							"legend_columns": [
								"avg",
								"min",
								"max",
								"value",
								"sum"
							],
							"time": {},
							"type": "timeseries",
							"requests": [
								{
									"queries": [
										{
											"data_source": "metrics",
											"name": "query1",
											"query": "sum:trace.rack.request.hits{env:im-workshop,service:store-frontend}.as_count()"
										}
									],
									"response_format": "timeseries",
									"display_type": "bars"
								},
								{
									"queries": [
										{
											"data_source": "metrics",
											"name": "query1",
											"query": "sum:trace.rack.request.errors{env:im-workshop,service:store-frontend}.as_count()"
										}
									],
									"response_format": "timeseries",
									"style": {
										"palette": "warm"
									},
									"display_type": "bars"
								}
							],
							"yaxis": {
								"scale": "linear",
								"include_zero": true,
								"label": "",
								"min": "auto",
								"max": "auto"
