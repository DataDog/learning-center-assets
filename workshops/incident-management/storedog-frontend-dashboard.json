								{
									"formulas": [
										{
											"alias": "",
											"formula": "query1"
										}
									],
									"queries": [
										{
											"data_source": "metrics",
											"name": "query1",
											"query": "avg:trace.rack.request{env:im-workshop,service:store-frontend,resource_name:spree::homecontroller_index}"
										}
